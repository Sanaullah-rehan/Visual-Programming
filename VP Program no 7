using System;

class Animal
{
    public void Elephent()
    {
        Console.WriteLine("Eating...");
    }
}

class Dog : Animal
{
    public void Lion()
    {
        Console.WriteLine("hunting");
    }
}

class Program
{
    static void Main()
    {
        Dog d = new Dog();
        d.Elephant();
        d.Lion();
    }
}
